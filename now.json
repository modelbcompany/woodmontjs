{
  "name": "woodmontjs",
  "version": 2,
  "alias": "woodmontjs.modelb.now.sh",
  "github": {
    "autoAlias": true,
    "enabled": true,
    "silent": false
  },
  "scope": "modelb",
  "env": {
    "apiToken": "@apitoken",
    "companyCode": "@companycode",
    "marketingAPIKey": "@marketingapikey",
    "propertyId": "@propertyid"
  },
  "build": {
    "env": {
      "apiToken": "@apitoken",
      "companyCode": "@companycode",
      "marketingAPIKey": "@marketingapikey",
      "propertyId": "@propertyid"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "regions": [
    "iad1"
  ],
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/docs/react-components-elements-and-instances",
      "destination": "https://reactjs.org/blog/2015/12/18/react-components-elements-and-instances.html",
      "statusCode": 308
    },
    {
      "source": "/docs/storybook-config",
      "destination": "https://medium.com/storybookjs/declarative-storybook-configuration-49912f77b78",
      "statusCode": 308
    },
    {
      "source": "/repo",
      "destination": "https://github.com/modelbcompany/woodmontjs",
      "statusCode": 308
    }
  ]
}